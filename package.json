{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "build:dev": "env-cmd -f env/.env.development.js npm run build",
    "build:prod": "env-cmd -f env/.env.production.js npm run build",
    "eject": "craco eject",
    "extract-intl": "babel-node --presets @babel/preset-env -- ./internals/scripts/extract-intl.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "sort-package": "sort-package-json package.json",
    "start": "craco start",
    "start:dev": "env-cmd -f env/.env.development.js npm run start",
    "start:prod": "env-cmd -f env/.env.production.js npm run start",
    "test": "craco test"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-empty-pattern": 0,
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/**/services.ts",
      "!<rootDir>/src/**/services/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleNameMapper": {
      "^src/(.*)": [
        "<rootDir>/src/$1"
      ],
      "^app/(.*)": "<rootDir>/src/app/$1",
      "^pages/(.*)": "<rootDir>/src/app/pages/$1",
      "^assets/(.*)": "<rootDir>/src/assets/$1",
      "^config/(.*)": "<rootDir>/src/config/$1",
      "^styles/(.*)": "<rootDir>/src/styles/$1"
    }
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "react-error-overlay": "6.0.9",
    "swc-loader": "0.1.15"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "add": "^2.0.6",
    "antd": "^4.20.1",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "immutable": "^4.0.0",
    "intl": "^1.2.5",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intl": "^2.9.0",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/node": "^7.16.5",
    "@babel/plugin-proposal-optional-chaining": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@craco/craco": "^6.4.3",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^3.0.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.0.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.4",
    "@types/react-intl": "^2.3.18",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "babel-plugin-react-intl": "^8.2.25",
    "craco-alias": "^3.0.1",
    "craco-swc": "^0.1.3",
    "env-cmd": "^10.1.0",
    "node-sass": "^6.0.1",
    "plop": "^3.0.5",
    "preconnect-html-webpack-plugin": "^1.0.0",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "react-error-overlay": "6.0.9",
    "shelljs": "^0.8.4",
    "sort-package-json": "^1.55.0",
    "swc-loader": "0.1.15"
  }
}
